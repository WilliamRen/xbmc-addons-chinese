<?xml version="1.0" encoding="utf-8"?>
<SubPath>
   <customPath>
      <!---                       
      是否设定指定的目录存放字幕文件，其值只能为True或False（注意大小写）
       -->
      <hasPath>False</hasPath>
      <!--
      绝对路径名，在不同的系统下请注意路径符号
      -->
      <pathName>E:\新建文件夹</pathName>
      <!--
      保存字幕的目标系统编码，如果是windows，为gbk
      如果是linux，可能为utf8，gbk，gb2312，也有可能是en_US/ISO-8859-1,请仔细参考LANG这个环境变量
      -->
      <destCode>gbk</destCode>
   </customPath>
</SubPath>